	a ex:Course ;
	ex:cSubject "PHIL" ;
	ex:cNum 327 ;
	ex:cName "KINDS OF MINDS" ;
	ex:cDescription "Please see UGRD Calendar" .

ex:26581
	a ex:Course ;
	ex:cSubject "PHIL" ;
	ex:cNum 328 ;
	ex:cName "Intermediate Philosophy of Science" ;
	ex:cDescription "Please see UGRD Calendar" .

ex:26584
	a ex:Course ;
	ex:cSubject "PHIL" ;
